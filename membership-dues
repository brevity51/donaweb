                <label for="revenue">Your annual revenue is:</label>
                <select id="revenue">
                  <option value="">Select revenue range</option>
                  <option value="500">$0 - $500,000</option>
                  <option value="1000">$500,001 - $1,000,000</option>
                  <option value="1500">$1,000,001 - $5,000,000</option>
                  <option value="3000">$5,000,001 - $10,000,000</option>
                  <option value="7500">$10,000,001 - $25,000,000</option>
