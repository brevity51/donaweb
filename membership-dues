                </select>

                <p id="dues">Your dues for this fiscal year would be: $0</p>
              </div>

              <script>
                const revenueDropdown = document.getElementById('revenue');
                const fiscalEndDropdown = document.getElementById('fiscal-end');
                const duesDisplay = document.getElementById('dues');

                function calculateDues() {
                  const revenue = parseFloat(revenueDropdown.value);
                  const fiscalEndMonth = parseInt(fiscalEndDropdown.value);

                  // Ensure both fields are selected before calculating dues
                  if (isNaN(revenue) || isNaN(fiscalEndMonth)) {
                    duesDisplay.style.display = 'none'; // Hide dues display if any field is not selected
                    return;
                  }

                  // Get current month
                  const currentMonth = new Date().getMonth();

                  // Calculate the remaining months in the fiscal year
                  let monthsRemaining;
                  if (fiscalEndMonth === currentMonth) {
                    // If the user selects the current month, show full dues
                    monthsRemaining = 12;
                  } else if (fiscalEndMonth > currentMonth) {
                    monthsRemaining = fiscalEndMonth - currentMonth;
                  } else {
                    monthsRemaining = 12 - (currentMonth - fiscalEndMonth);
                  }

                  // Calculate prorated dues based on months remaining
                  const proratedDues = (revenue * monthsRemaining) / 12;

                  // Show and update the dues display
                  duesDisplay.textContent = `Your dues for this fiscal year would be: $${proratedDues.toFixed(2)}`;
                  duesDisplay.style.display = 'block'; // Show the dues display
                }


                // Event listeners for dropdown changes
                revenueDropdown.addEventListener('change', calculateDues);
                fiscalEndDropdown.addEventListener('change', calculateDues);

                // GA4 Event Tracking
                document.getElementById('fiscal-end').addEventListener('change', function() {
                  gtag('event', 'select_fiscal_end_month', {
                    'event_category': 'FiscalYear',
                    'event_label': this.options[this.selectedIndex].text,
                    'value': this.value
                  });
                });

                document.getElementById('revenue').addEventListener('change', function() {
                  gtag('event', 'select_revenue_range', {
                    'event_category': 'Revenue',
                    'event_label': this.options[this.selectedIndex].text,
                    'value': this.value
                  });
                });

                // Initial calculation hidden on page load until selections are made
                calculateDues();
              </script>
