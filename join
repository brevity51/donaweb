          <script>
            async function getIpAndLoadWidget() {
              // load data about your customer from db-ip.com
              const result = await fetch('https://api.db-ip.com/v2/free/self');
              const data = await result.json();
              const {
                ipAddress,
                continentName,
                countryName,
                cityName
              } = data || {};

              // construct the hidden fields string
              const hiddenFields = `hubspot_utk=,hubspot_page_name=,hubspot_page_url=,country=${countryName},ip=${ipAddress},continent=${continentName},city=${cityName}`;

              // update the data-tf-hidden attribute with the hidden fields
              const wrapper = document.querySelector('#wrapper');
              wrapper.setAttribute('data-tf-hidden', hiddenFields);
            }

            getIpAndLoadWidget();
          </script>
