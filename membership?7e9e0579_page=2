            <div class="w-embed w-iframe w-script"><button id="joinButton" data-tf-popup="Hz8RQTIy" data-tf-opacity="100" data-tf-size="100" data-tf-iframe-props="title=Join as a 1% for the Planet Member" data-tf-transitive-search-params data-tf-medium="snippet" data-tf-hidden="utm_source=membershipBanner_popUp,hubspot_utk=,hubspot_page_name=,hubspot_page_url=,country=,ip=,continent=,city=" data-tf-hubspot class="button-primary margin-left-5px centered-button-text membership">Join the movement</button>
              <script src="//embed.typeform.com/next/embed.js"></script>
              <script>
                async function getIpAndLoadPopup() {
                  // load data about your customer from db-ip.com
                  const result = await fetch('https://api.db-ip.com/v2/free/self');
                  const data = await result.json();
                  const {
                    ipAddress,
                    continentName,
                    countryName,
                    city
                  } = data || {};

                  // construct the hidden fields string
                  const hiddenFields = `utm_source=membershipBanner_popUp,hubspot_utk=,hubspot_page_name=,hubspot_page_url=,country=${countryName},ip=${ipAddress},continent=${continentName},city=${city}`;

                  // update the data-tf-hidden attribute with the hidden fields
                  const joinButton = document.querySelector('#joinButton');
                  joinButton.setAttribute('data-tf-hidden', hiddenFields);
                }

                getIpAndLoadPopup();
              </script>
            </div><a href="https://directories.onepercentfortheplanet.org/?accountType=business" class="button-primary margin-left-5px centered-button-text membership w-button">See who&#x27;s already joined</a>
